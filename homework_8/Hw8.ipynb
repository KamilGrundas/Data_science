      "source": [
        "# Test the trained neural network on 10 images. Select 5 random images from the test sample\n",
        "# and input them into the neural network.\n",
        "# Output the image and display the neural network's response next to it.\n",
        "# Does the neural network make mistakes, and if so, how often?\n",
        "\n",
        "# Generating a list of unique, random numbers\n",
        "unique_random_numbers = random.sample(range(0, len(x_test)), 10)\n",
        "\n",
        "# Setting up the grid layout\n",
        "fig, axes = plt.subplots(2, 5, figsize=(15, 3))\n",
        "\n",
        "# Iterating over unique random numbers and displaying images with predictions\n",
        "mistakes = 0\n",
        "for ax, sample_num in zip(axes.flatten(), unique_random_numbers):\n",
        "    x1 = x_test[sample_num].reshape(28, 28)\n",
        "    prediction = neural_net(np.array([x_test[sample_num]]))\n",
        "    predicted_label = np.argmax(prediction, axis=1)[0]\n",
        "    true_label = y_test[sample_num]\n",
        "\n",
        "    ax.imshow(x1, cmap='gray')\n",
        "    ax.set_title(f\"True: {true_label}, Pred: {predicted_label}\")\n",
        "    ax.axis('off')\n",
        "\n",
        "    if predicted_label != true_label:\n",
        "        mistakes += 1\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "print(f\"The neural network made {mistakes} mistakes out of 10 images.\")\n"
      ],
