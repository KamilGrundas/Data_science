      "source": [
        "# Plot the change in accuracy and loss as a function of step\n",
        "# If done correctly, accuracy should increase and loss should decrease.\n",
        "plt.subplot(1, 2, 1)\n",
        "plt.plot(displays_steps, loss_history, color='g')\n",
        "plt.grid(True)\n",
        "plt.legend([\"loss history\"])\n",
        "plt.subplot(1, 2, 2)\n",
        "plt.plot(displays_steps, accuracy_history, color='b')\n",
        "plt.grid(True)\n",
        "plt.legend([\"accuracy history\"])"
      ],
